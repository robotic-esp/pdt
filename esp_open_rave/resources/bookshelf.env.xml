<environment>
  <!-- Robot -->
  <robot name="BarrettWAM-dual" file="robots/barrettwam-dual.robot.xml">
    <translation>-0.4 0.0 0.001</translation>
  </robot>

  <!-- Floor -->
  <kinbody name="floor">
    <body type="static">
      <translation>0 0 0</translation>
      <geom type="box">
        <extents>2.5 2.5 0.005</extents>
        <translation>0 0 -0.005</translation>
        <diffuseColor>0.6 0.6 0.6</diffuseColor>
      	<ambientColor>0.5 0.5 0.5</ambientColor>
      </geom>
    </body>
  </kinbody>

  <!-- Bookshelf -->
  <kinbody name="ikeashelf" file="objects/ikeashelf.kinbody.xml">
    <translation>0.5 0.0 0.0</translation>
    <rotationaxis>0 0 1 180.0</rotationaxis>
  </kinbody>
</environment>
