{
    "Experiment": {
        "executable": "benchmark",
        "planners": [
            "RRTConnect",
            "RRT*"
        ],
        "context": "WallGap",
        "numRuns": 100,
        "logFrequency": 10000,
        "loadDefaultContextConfig": true,
        "loadDefaultPlannerConfig": true
    },
    "Contexts": {
        "FourRooms": {
            "type": "FourRooms",
            "start": [0.5, -0.5],
            "goal": [0.5, 0.5],
            "maxTime": 1.0,
            "dimensions": 2,
            "boundarySideLengths" : 2,
            "collisionCheckResolution": 0.000001,
            "wallThickness": 0.5,
            "gapWidth": 0.1
        },
        "WallGap": {
            "type": "WallGap",
            "start": [-0.5, 0.0],
            "goal": [0.5, 0.0],
            "maxTime": 0.1,
            "dimensions": 2,
            "boundarySideLengths" : 2,
            "collisionCheckResolution": 0.001,
            "wallWidth": 1.7,
            "wallThickness": 0.4,
            "gapOffset": 0.1,
            "gapWidth": 0.01
        }
    },
    "Planners": {
        "TBD*": {
            "type": "TBDstar",
            "batchSize": 100,
            "repairBackwardSearch": true
        },
        "TBD* Light": {
            "type": "TBDstar",
            "batchSize": 100,
            "repairBackwardSearch": false
        },
        "SBITstarRegression": {
            "type": "BITstarRegression",
            "useKNearest": false,
            "rewireFactor": 1.1,
            "samplesPerBatch": 100,
            "enablePruning": false,
            "pruningThreshold": 0.1,
            "useJustInTimeSampling": false,
            "dropSamplesOnPrune": false,
            "stopOnSolutionImprovement": false,
            "initialInflation": 1000000,
            "inflationParameter": 10,
            "truncationParameter": 5
        }
    }
}
