<environment>
  <!-- Robot -->
  <robot name="table">
    <kinbody name="ikeatable" file="objects/ikeatable.kinbody.xml">
	    <rotationaxis>0 0 1 90</rotationaxis>
    </kinbody>
  </robot>

  <!-- Environment -->
  <kinbody name="house">
    <body name="floor" type="static">
      <geom type="box">
        <translation>0.0 0.0 -0.005</translation>
        <extents>4.0 4.0 0.005</extents>
        <diffusecolor>0.6 0.6 0.6</diffusecolor>
      	<ambientcolor>0.5 0.5 0.5</ambientcolor>
      </geom>
    </body>
    <body name="walls" type="static">
      <offsetfrom>floor</offsetfrom>
      <geom type="box">
        <translation>0.0 -3.5 1.25</translation>
        <extents>0.1 0.5 1.25</extents>
        <diffusecolor>0.2 0.5 1.0</diffusecolor>
      </geom>
      <geom type="box">
        <translation>0.0 0.0 1.25</translation>
        <extents>0.1 2.0 1.25</extents>
        <diffusecolor>0.2 0.5 1.0</diffusecolor>
      </geom>
      <geom type="box">
        <translation>0.0 3.5 1.25</translation>
        <extents>0.1 0.5 1.25</extents>
        <diffusecolor>0.2 0.5 1.0</diffusecolor>
      </geom>
      <geom type="box">
        <translation>-3.5 0.0 1.25</translation>
        <extents>0.5 0.1 1.25</extents>
        <diffusecolor>0.2 0.5 1.0</diffusecolor>
      </geom>
      <geom type="box">
        <translation>1.0 0.0 1.25</translation>
        <extents>3.0 0.1 1.25</extents>
        <diffusecolor>0.2 0.5 1.0</diffusecolor>
      </geom>
    </body>
  </kinbody>
</environment>
